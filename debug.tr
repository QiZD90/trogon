for let i in 0 .. 4 {
	for let j in 0 .. 20 {
		if (i * 20 + j) % 2 != 0 {
			continue;
		}

		print(i * 20 + j);
	}
}

for let i in 0 .. 10 {
	let str = '          ';
	if i % 2 != 0 {
		continue;
	}

	for let j in 0 .. 10 {
		str[j] = '#';

		if j >= i {
			break;
		}
	}

	print(str);
}

let f = function() {return 3; return 11; 77};
print(f());

function a() {
	for let i in 0 .. 11 {
		return 'xyz';
	}
}

print(a());

